union option a = 
    | Some: a
    | None

nominal tensor el dim = `(tensor_init `el `dim ``el ``dim)
and union tensor_option el dim =
    | Some: tensor el dim
    | None
and nominal tensor2d el a b = tensor el (a * b)

prototype bind m a : forall b. m a -> (a -> m b) -> m b

instance bind option a : forall b. x f =
    match x with
    | Some: x => (Some: f x)
    | None => None

inl rec f x = x
and inl g x = f x
and let g x = x

